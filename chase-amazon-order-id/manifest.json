{
  "name": "Chase Amazon Order Id Scraper",
  "description": "Scrape Amazon order ids from chase.com",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://*.chase.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
